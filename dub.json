{
    "name": "electronvolt",
    "description": "FPS with physics based puzzles",
    "homepage": "https://github.com/gecko0307/electronvolt",
    "license": "Boost",
    "authors": [
        "Timur Gafarov"
    ],
    
    "dependencies": {
        "dagon": "~master",
        "dagon:imgui": "~master",
        "dagon:newton": "~master",
        "bindbc-soloud": "~>0.1"
    },
    
    "importPaths": [
        "src"
    ],
    
    "buildRequirements":[
        "allowWarnings"
    ],
    
    "sourcePaths": ["src"],
    "mainSourceFile": "src/main.d",
    "sourceFiles-windows": ["resource.res"],
    
    "configurations": [
        {
            "name": "application",
            "targetType": "executable"
        }
    ]
}
